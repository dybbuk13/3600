#include <stdio.h>
#include <unistd.h>
#include <pthread.h>
void inc_n(int *n)
{
   /* increment n to 100 */
   while(++(*n) < 1000000000);
}

int main()
{
    int x = 0, y = 0, i;
    pthread_t tid[3];
    printf("x: %d, y: %d\n", x, y);

   // for(i = 0; i < 3; i++){
      pthread_create(&tid[0], NULL, inc_n, &x);
   // }
   // for(i = 0; i < 3; i++){
  //    pthread_join(tid[i], (void **
    printf("x increment finished\n");

    inc_n(&y);
    printf("y increment finished\n");
    printf("x: %d, y: %d\n", x, y);

    return 1;
}
